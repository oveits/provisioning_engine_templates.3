<h1>Listing my_templates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_templates.each do |my_template| %>
      <tr>
        <td><%= my_template.name %></td>
        <td><%= my_template.type %></td>
        <td><%= my_template.body %></td>
        <td><%= link_to 'Show', my_template %></td>
        <td><%= link_to 'Edit', edit_my_template_path(my_template) %></td>
        <td><%= link_to 'Destroy', my_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New My template', new_my_template_path %>
